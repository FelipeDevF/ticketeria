  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./orm-datasource.ts",
    "typeorm:windows": ".\\node_modules\\.bin\\ts-node -r tsconfig-paths\\register .\\node_modules\\typeorm\\cli.js -d .\\orm-datasource.ts",
    "typeorm:init": "npx typeorm-ts-node-commonjs migration:generate src/database/migrations/InitialMigration -d ./orm-datasource.ts",
    "typeorm:init:windows": "npx typeorm-ts-node-commonjs migration:generate src/database/migrations/InitialMigration -d .\\orm-datasource.ts",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "db:setup": "npm run migrate && npm run seed",
    "dev:setup": "npm run db:setup && npm run start:dev",
    "generate-pdf": "node scripts/generate-pdf.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "4",
    "typeorm": "^0.3.25",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/express": "^5.0.0",
    "@types/node": "^20.3.1",
    "eslint": "^9.31.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
